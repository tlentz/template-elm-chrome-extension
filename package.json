{
  "name": "template-elm-chrome-extension",
  "version": "1.0.0",
  "description": "A template repository for building a chrome extension with webpack and elm.",
  "main": "TemplateElmChromeExtension/app.html",
  "scripts": {
    "debug": "webpack --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js --watch",
    "live": "webpack-dev-server --config webpack.live.js --open --hot --colors",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run live",
    "restart": "rm -rf elm-stuff && npm start",
    "test": "elm-test",
    "prod": "npm run build && zip -r ./TemplateElmChromeExtension.zip ./TemplateElmChromeExtension"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/tlentz/template-elm-chrome-extension.git"
  },
  "bugs": {
    "url": "https://github.com/tlentz/template-elm-chrome-extension/issues"
  },
  "homepage": "https://github.com/tlentz/template-elm-chrome-extension#readme",
  "keywords": [
    "template",
    "chrome-extension",
    "elm",
    "webpack"
  ],
  "author": "Tyler Lentz",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chrome-extension-async": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-transform-classes": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "axios": "^0.21.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "elm": "^0.19.1-3",
    "elm-hot-webpack-loader": "^1.1.6",
    "elm-minify": "^2.0.4",
    "elm-test": "^0.19.1-revision2",
    "elm-webpack-loader": "^6.0.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "fibers": "^4.0.2",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jquery": "^3.5.1",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "^4.41.2",
    "webpack-chrome-extension-reloader": "^1.3.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  }
}
