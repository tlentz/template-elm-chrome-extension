{
  "manifest_version": 2,
  "name": "template-elm-chrome-extension",
  "short_name": "template-elm-chrome-extension",
  "version": "#version#",
  "description": "#description#",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": true
  },
  "author": "Tyler Lentz",
  "permissions": ["activeTab"]
}
